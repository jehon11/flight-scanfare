<% if params[:chart_id].nil? %>
<div class="selected-city-image-container" style="background-image: url(<%= asset_path((" #{@city}.jpg").strip) %>;"></div>
<%else%>
<div class="graph-card" style="background-image: linear-gradient(rgba(255,255,255,0.8), rgba(255,255,255,0.7)), url(<%= asset_path((" #{@city}.jpg").strip) %>;">
  <div class="graph">
    <% average = @deal_chart.get_historical.map(&:last).sum/@deal_chart.get_historical.map(&:last).size %>
    <% @average_series = @deal_chart.get_historical.map{|a| [a[0],average] } %>
    <!--
     area_chart @deal_chart.get_historical, min:  @deal_chart.get_historical.map(&:last).min, max:  @deal_chart.get_historical.map(&:last).max,  points: false -->
    <%= line_chart  [
  {name: "Price", data: @deal_chart.get_historical}] %>
  </div>
  <%end%>
